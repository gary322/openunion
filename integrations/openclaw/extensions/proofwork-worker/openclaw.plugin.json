{
  "id": "proofwork-worker",
  "name": "Proofwork Worker",
  "description": "Runs a Proofwork universal worker loop in the background (no OpenClaw core changes).",
  "version": "0.1.1",
  "configSchema": {
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "apiBaseUrl": { "type": "string", "minLength": 1 },
      "enabled": { "type": "boolean" },
      "autoStart": { "type": "boolean" },
      "browserProfile": { "type": "string", "minLength": 1 },
      "resetBrowserProfileOnStart": { "type": "boolean" },
      "supportedCapabilityTags": {
        "type": "array",
        "items": { "type": "string", "minLength": 1 },
        "minItems": 1
      },
      "minPayoutCents": { "type": "integer", "minimum": 0 },
      "preferCapabilityTag": { "type": "string", "minLength": 1 },
      "requireTaskType": { "type": "string", "minLength": 1 },
      "canaryPercent": { "type": "integer", "minimum": 0, "maximum": 100 },
      "pollIntervalMs": { "type": "integer", "minimum": 250, "maximum": 60000 },
      "originEnforcement": { "type": "string", "enum": ["strict", "off"] },
      "noLogin": { "type": "boolean" },
      "valueEnvAllowlist": { "type": "array", "items": { "type": "string", "minLength": 1 } },
      "extraAllowedOrigins": { "type": "array", "items": { "type": "string", "minLength": 1 } },
      "jobTimeBudgetSec": { "type": "integer", "minimum": 10, "maximum": 3600 },
      "httpMaxBytes": { "type": "integer", "minimum": 1024, "maximum": 50000000 },
      "artifactMaxBytes": { "type": "integer", "minimum": 1024, "maximum": 500000000 },
      "ffmpegMaxDurationSec": { "type": "integer", "minimum": 1, "maximum": 600 },
      "logLevel": { "type": "string", "enum": ["info", "debug"] },
      "allowTaskTypes": { "type": "array", "items": { "type": "string", "minLength": 1 } },
      "denyTaskTypes": { "type": "array", "items": { "type": "string", "minLength": 1 } },
      "allowOrigins": { "type": "array", "items": { "type": "string", "minLength": 1 } },
      "denyOrigins": { "type": "array", "items": { "type": "string", "minLength": 1 } },
      "openclawBin": { "type": "string", "minLength": 1 },
      "workerScriptPath": { "type": "string", "minLength": 1 },
      "payoutChain": { "type": "string", "enum": ["base"] },
      "payoutAddress": { "type": "string", "minLength": 1 },
      "payoutSignature": { "type": "string", "minLength": 1 },
      "dangerouslyEnableOpenclawAgentSummarize": { "type": "boolean" }
    },
    "required": []
  },
  "uiHints": {
    "apiBaseUrl": { "label": "Proofwork API Base URL", "placeholder": "https://proofwork.example" },
    "enabled": { "label": "Enabled" },
    "autoStart": { "label": "Auto-start worker" },
    "browserProfile": { "label": "Browser profile", "placeholder": "proofwork-worker" },
    "resetBrowserProfileOnStart": { "label": "Reset browser profile on start", "advanced": true },
    "supportedCapabilityTags": { "label": "Supported capability tags" },
    "minPayoutCents": { "label": "Min payout (cents)", "advanced": true },
    "preferCapabilityTag": { "label": "Prefer capability tag", "advanced": true },
    "requireTaskType": { "label": "Require task type", "advanced": true },
    "canaryPercent": { "label": "Canary percent", "advanced": true },
    "pollIntervalMs": { "label": "Poll interval (ms)", "advanced": true },
    "originEnforcement": { "label": "Origin enforcement" },
    "noLogin": { "label": "Block login flows" },
    "valueEnvAllowlist": { "label": "Allowed value_env vars", "advanced": true },
    "extraAllowedOrigins": { "label": "Extra allowed origins", "advanced": true },
    "jobTimeBudgetSec": { "label": "Job time budget (sec)", "advanced": true },
    "httpMaxBytes": { "label": "HTTP max bytes", "advanced": true },
    "artifactMaxBytes": { "label": "Artifact max bytes", "advanced": true },
    "ffmpegMaxDurationSec": { "label": "ffmpeg max duration (sec)", "advanced": true },
    "logLevel": { "label": "Log level", "advanced": true },
    "allowTaskTypes": { "label": "Allow task types", "advanced": true },
    "denyTaskTypes": { "label": "Deny task types", "advanced": true },
    "allowOrigins": { "label": "Allow origins", "advanced": true },
    "denyOrigins": { "label": "Deny origins", "advanced": true },
    "openclawBin": { "label": "openclaw binary", "advanced": true },
    "workerScriptPath": { "label": "Worker script path override", "advanced": true },
    "payoutChain": { "label": "Payout chain", "advanced": true },
    "payoutAddress": { "label": "Payout address", "advanced": true, "placeholder": "0x..." },
    "payoutSignature": { "label": "Payout signature", "advanced": true, "placeholder": "0x..." },
    "dangerouslyEnableOpenclawAgentSummarize": { "label": "Enable OpenClaw agent summarize (dangerous)", "advanced": true }
  }
}
